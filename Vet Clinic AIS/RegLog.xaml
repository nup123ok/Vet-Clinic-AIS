<Window x:Class="Vet_Clinic_AIS.RegLog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vet_Clinic_AIS"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Вход" Height="300" Width="400">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Margin="5" HorizontalAlignment="Left" Name="LogLabel">Введите Логин</TextBlock>
            <TextBox Name="LoginTb" Margin="5" HorizontalAlignment="Left" Width="200" Height="30"/>
            <TextBlock Margin="5" HorizontalAlignment="Left">Введите Пароль</TextBlock>
            <PasswordBox Name="PasswordTb" Margin="5" HorizontalAlignment="Left" Width="200" Height="30"/>
            <Button Margin="5" Click="Enter_Click" Width="200" Height="35" Background="#ff6d00">Войти</Button>
        </StackPanel>
    </Grid>
</Window>
